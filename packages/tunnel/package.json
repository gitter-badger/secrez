  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/secrez/secrez.git"
  },
  "contributors": [
    "Roman Shtylman <shtylman@gmail.com>",
    "Francesco Sullo <francesco@sullo.co>"
